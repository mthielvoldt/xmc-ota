  "version": "0.2.0",
  "configurations": [
    {
      "name": "standalone",
      "type": "cortex-debug",
      "servertype": "jlink",
      "request": "launch",
      "serverpath": "JLinkGDBServerCLExe",
      "cwd": "${workspaceFolder}",
      "executable": "build/app_standalone.elf",
      "device": "XMC4700-2048",
      "svdFile": "firmware/port/XMC4/mtb-xmclib-cat3/CMSIS/Infineon/SVD/XMC4700.svd",
      "showDevDebugOutput": "vscode",
    },
    {
      "name": "myBoot",
      "type": "cortex-debug",
      "servertype": "jlink",
      "request": "launch",
      "serverpath": "JLinkGDBServerCLExe",
      "cwd": "${workspaceFolder}",
      "executable": "build/bootloader.elf",
      "device": "XMC4700-2048",
      "svdFile": "firmware/port/XMC4/mtb-xmclib-cat3/CMSIS/Infineon/SVD/XMC4700.s",
      "showDevDebugOutput": "vscode",
      "preLaunchCommands": [ "add-symbol-file images/app_v2.elf" ]
    },
    {
      "name": "myBoot-attach",
      "type": "cortex-debug",
      "servertype": "jlink",
      "request": "attach",
      "serverpath": "JLinkGDBServerCLExe",
      "cwd": "${workspaceFolder}",
      "executable": "build/bootloader.elf",
      "device": "XMC4700-2048",
      "svdFile": "firmware/port/XMC4/mtb-xmclib-cat3/CMSIS/Infineon/SVD/XMC4700.s",
      "showDevDebugOutput": "vscode",
      "preAttachCommands": [ "add-symbol-file images/app_v2.elf" ]
    },
  ]
}